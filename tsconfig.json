{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@site/*": [
        "./*"
      ],
      "@theme/*": [
        "node_modules/@docusaurus/theme-classic/*",
        "./src/theme/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "docusaurus.config.ts",
    "sidebars.ts"
  ],
  "exclude": [
    "node_modules",
    "build",
    ".docusaurus",
    "backend"
  ]
}